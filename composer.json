{
	"name": "patriceckhart/typo3-cms-boilerplate",
	"description" : "A boilerplate for building TYPO3 CMS websites",
	"license": "GPL-2.0-or-later",
	"config": {
		"platform": {
			"php": "7.2"
		},
		"sort-packages": true
	},
	"repositories": [
		{
			"type": "path",
			"url": "public/typo3conf/ext/provider"
		},
		{
			"type": "path",
			"url": "public/typo3conf/ext/base"
		}
	],
	"require": {
		"typo3/cms-about": "^10.2",
		"typo3/cms-backend": "^10.2",
		"typo3/cms-belog": "^10.2",
		"typo3/cms-beuser": "^10.2",
		"typo3/cms-core": "^10.2",
		"typo3/cms-dashboard": "^10.2",
		"typo3/cms-extbase": "^10.2",
		"typo3/cms-extensionmanager": "^10.2",
		"typo3/cms-felogin": "^10.2",
		"typo3/cms-filelist": "^10.2",
		"typo3/cms-fluid": "^10.2",
		"typo3/cms-fluid-styled-content": "^10.2",
		"typo3/cms-form": "^10.2",
		"typo3/cms-frontend": "^10.2",
		"typo3/cms-impexp": "^10.2",
		"typo3/cms-info": "^10.2",
		"typo3/cms-install": "^10.2",
		"typo3/cms-recordlist": "^10.2",
		"typo3/cms-rte-ckeditor": "^10.2",
		"typo3/cms-seo": "^10.2",
		"typo3/cms-setup": "^10.2",
		"typo3/cms-sys-note": "^10.2",
		"typo3/cms-t3editor": "^10.2",
		"typo3/cms-tstemplate": "^10.2",
		"typo3/cms-viewpage": "^10.2",
		"patric-eckhart/base": "0.1.x-dev",
		"patric-eckhart/provider": "0.1.x-dev"
	}
}